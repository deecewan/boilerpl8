{
  "presets": [
    "es2015",
    "es2016",
    "react",
    "stage-0"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  },
  "ignore": "node_modules"
}