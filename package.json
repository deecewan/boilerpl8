{
  "name": "boilerpl8",
  "version": "0.0.1",
  "description": "Boilerplate for Express and React apps",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "build": "npm run clean && npm run build:client && npm run build:index && npm run build:server",
    "build:client": "webpack --config webpack.config.babel.js",
    "build:index": "babel index.js -o index-compiled.js",
    "build:server": "babel server -d out",
    "clean": "rimraf index-compiled.js out dist/bundle.js",
    "serve:dash": "NODE_ENV=development webpack-dashboard -- babel-node index.js",
    "serve:dev": "NODE_ENV=development babel-node index.js",
    "start": "NODE_ENV=production forever index-compiled.js",
    "test": "nyc mocha --require babel-register ./test/**/*_test.js"
  },
  "author": "David Buchan-Swanson (https://deecewan.github.io)",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "chokidar": "^1.6.0",
    "dotenv": "^2.0.0",
    "eslint-loader": "^1.5.0",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "morgan": "^1.7.0",
    "pg": "^6.1.0",
    "radium": "^0.18.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.24.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack-dashboard": "^0.1.6"
  },
  "nyc": {
    "include": [
      "server/**/*.js",
      "client/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
